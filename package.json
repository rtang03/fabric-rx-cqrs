{
  "name": "fabric-rx-cqrs",
  "version": "0.1.25",
  "description": "Library to empower Hyperledger Fabric application with CQRS-ES & Reactive Patterns",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "keywords": [
    "Hyperledger",
    "Fabric",
    "rxjs",
    "cqrs",
    "reactive",
    "event sourcing",
    "graphql"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rtang03/fabric-rx-cqrs.git"
  },
  "homepage": "https://github.com/rtang03/fabric-rx-cqrs#readme",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "test:eventstore": "yarn run build && mocha --timeout 20000 ./dist/service/eventstore.spec.js",
    "test:entity-cqrs": "yarn run build && mocha --timeout 20000 ./dist/cqrs/entity/entity.spec.js",
    "test:entity-command": "yarn run build && mocha --timeout 20000 ./dist/cqrs/entity-command/entity.spec.js",
    "test:entity-query": "yarn run build && mocha --timeout 20000 ./dist/cqrs/entity-query/entity.spec.js",
    "test": "yarn run build && mocha --reporter=xunit --reporter-options output=test-results/result.xml --timeout 20000 ./dist/cqrs/entity-query/entity.spec.js",
    "test:projection": "yarn run build && mocha --timeout 20000 ./dist/cqrs/projection/projection.spec.js",
    "test:counter": "yarn run build && mocha --timeout 20000 ./dist/example/repository.spec.js",
    "build": "npm run clean && tsc",
    "build:watch": "tsc -w",
    "clean": "rimraf dist",
    "addToWallet": "rimraf identity && ./node_modules/.bin/ts-node ./src/addToWallet.ts"
  },
  "engineStrict": true,
  "author": "Ross Tang",
  "license": "Apache-2.0",
  "dependencies": {
    "chalk": "^2.4.2",
    "dotenv": "^6.2.0",
    "fabric-client": "~1.4.1",
    "fabric-network": "~1.4.1",
    "graphql": "^14.3.1",
    "graphql-subscriptions": "^1.1.0",
    "graphql-type-json": "^0.3.0",
    "inversify": "^5.0.1",
    "inversify-inject-decorators": "^3.1.0",
    "js-yaml": "^3.12.0",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "prettyjson": "^1.2.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.2",
    "type-graphql": "^0.17.4",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bytebuffer": "^5.0.34",
    "@types/chai": "^4.1.7",
    "@types/graphql": "^14.2.0",
    "@types/graphql-type-json": "^0.3.0",
    "@types/lodash": "^4.14.119",
    "@types/mocha": "^5.2.5",
    "@types/node": "^8.10.39",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "prettier": "^1.17.1",
    "rimraf": "^2.6.3",
    "ts-loader": "^4.2.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.4.5"
  },
  "prettier": {
    "eslintIntegration": false,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "requirePragma": false,
    "insertPragma": false
  }
}
